{
    "name": "erwane/cakephp-geo",
    "description": "CakePHP 3.x geographic tools",
    "type": "cakephp-plugin",
    "authors": [
        {
            "name": "Erwane Breton",
            "email": "erwane@phea.fr"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.6",
        "cakephp/cakephp": "~3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "6.*"
    },
    "autoload": {
        "psr-4": {
            "Geo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Geo\\Test\\": "tests"
        }
    }
}
